[
  {
    "name": "redis_main",
    "kind": "redis",
    "conn_ref": "env:REFLEXFLOW_REDIS_CONN",
    "options": {
      "batch_size": 200,
      "timeout_ms": 500,
      "tls": false,
      "retries": 1,
      "mysql_table": "point_snapshot",
      "mysql_point_column": "point_id",
      "mysql_value_column": "value",
      "mysql_ts_column": null
    }
  },
  {
    "name": "mysql_main",
    "kind": "mysql",
    "conn_ref": "env:REFLEXFLOW_MYSQL_CONN",
    "options": {
      "batch_size": 500,
      "timeout_ms": 1000,
      "tls": false,
      "retries": 1,
      "mysql_table": "point_snapshot",
      "mysql_point_column": "point_id",
      "mysql_value_column": "value",
      "mysql_ts_column": null
    }
  }
]
